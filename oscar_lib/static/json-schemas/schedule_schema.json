{
    "definitions": {},
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://example.com/root.json",
    "type": "object",
    "title": "The Root Schema",
    "required": [
        "from",
        "to",
        "startMonth",
        "endMonth",
        "startWeekday",
        "endWeekday",
        "startHour",
        "endHour",
        "startMinute",
        "endMinute",
        "interval",
        "international"
    ],
    "properties": {
	    "from": {
            "$id": "#/properties/from",
            "type": ["string","null"],
            "title": "The From Schema",
            "format": "date-time",
            "default": "",
            "examples": [
                "2018-11-13T20:20:39+00:00"
            ]
        },
        "to": {
            "$id": "#/properties/to",
            "type": ["string","null"],
            "format": "date-time",
            "title": "The To Schema",
            "default": "",
            "examples": [
                "2018-11-13T20:20:39+00:00"
            ]
        },
        "startMonth": {
            "$id": "#/properties/startMonth",
            "type": "integer",
            "title": "The Startmonth Schema",
            "default": 1,
            "minimum": 1,
            "maximum": 12,
            "examples": [
                1
            ]
        },
        "endMonth": {
            "$id": "#/properties/endMonth",
            "type": "integer",
            "title": "The Endmonth Schema",
            "default": 12,
            "minimum": 1,
            "maximum": 12,
            "examples": [
                12
            ]
        },
        "startWeekday": {
            "$id": "#/properties/startWeekday",
            "type": "integer",
            "title": "The Startweekday Schema",
            "default": 1,
            "minimum": 1,
            "maximum": 7,
            "examples": [
                1
            ]
        },
        "endWeekday": {
            "$id": "#/properties/endWeekday",
            "type": "integer",
            "title": "The Endweekday Schema",
            "default": 7,
            "minimum": 1,
            "maximum": 7,
            "examples": [
                7
            ]
        },
        "startHour": {
            "$id": "#/properties/startHour",
            "type": "integer",
            "title": "The Starthour Schema",
            "default": 0,
            "minimum": 0,
            "maximum": 23,
            "examples": [
                0
            ]
        },
        "endHour": {
            "$id": "#/properties/endHour",
            "type": "integer",
            "title": "The Endhour Schema",
            "default": 23,
            "minimum": 0,
            "maximum": 23,
            "examples": [
                23
            ]
        },
        "startMinute": {
            "$id": "#/properties/startMinute",
            "type": "integer",
            "title": "The Startminute Schema",
            "default": 0,
            "minimum": 0,
            "maximum": 59,
            "examples": [
                0
            ]
        },
        "endMinute": {
            "$id": "#/properties/endMinute",
            "type": "integer",
            "title": "The Endminute Schema",
            "default": 59,
            "minimum": 0,
            "maximum": 59,
            "examples": [
                59
            ]
        },
        "interval": {
            "$id": "#/properties/interval",
            "type": "integer",
            "title": "The Interval Schema",
            "minimum": 0,
			"examples": [
                3600
            ]
        },
        "international": {
            "$id": "#/properties/international",
            "type": "boolean",
            "title": "The International Schema",
            "default": true,
            "examples": [
                true
            ]
        }
    }
}