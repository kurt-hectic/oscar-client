{
  "title": "Create station",
  "description": "to create station values",
  "required": [
    "latitude",
    "longitude",
    "altitude",
    "creation",
    "international",
    "manufacturer",
    "parametersObserved",
    "country",
    "utc",
    "wigosID"
  ],
  "type": "object",
  "properties": {
    "latitude": {
      "type": "number",
      "pattern": "/^-?([1-8]?[0-9]\\.{1}\\d{1,6}$|90\\.{1}0{1,6}$)/ || /^(\\d{1,3})\\s(\\d{1,3})\\s(\\d{1,3})[N|S]/"
    },
    "longitude": {
      "type": "number",
      "pattern": "/^(\\+|-)?(\\d\\.\\d{1,6}|[1-9]\\d\\.\\d{1,6}|1[1-7]\\d\\.\\d{1,6}|180\\.0{1,6})$/ || /^(\\d{1,3})\\s(\\d{1,3})\\s(\\d{1,3})[E|W]/"
    },
    "altitude": {
      "type": "integer",
      "pattern": "/^[0-9]+$/"
    },
    "creation": {
      "type": "date",
      "pattern": "/^([0]?[1-9]|[1|2][0-9]|[3][0|1])[./-]([0]?[1-9]|[1][0-2])[./-]([0-9]{4}|[0-9]{2})$/"
    },
    "international": {
      "type": "boolean"
    },
    "manufacturer": {
      "type": "string"
    },
    "parametersObserved": {
      "type": "string",
      "pattern": "/^(\\d+)(\\s*,\\s*\\d+)*$/"
    },
    "operationalStatus": {
      "type": "string"
    },
    "realTime": {
      "type": "boolean"
    },
    "affiliations": {
      "type": "string",
      "pattern": "/^(\\d+)(\\s*,\\s*\\d+)*$/"
    },
    "observationsFrequency": {
      "type": "string",
      "pattern": "/^(\\d+)(\\s*,\\s*\\d+)*$/"
    },
    "internationalReportingFrequency": {
      "type": "string",
      "pattern": "/^(\\d+)(\\s*,\\s*\\d+)*$/"
    },
    "country": {
      "type": "string"
    },
    "utc": {
      "type": "string"
    },
    "wigosID": {
      "type": "string",
      "pattern": "/^\\d{1}-\\d{2}-\\d{1,2}-\\d{16}$/"
    },
    "supervisingOrganization": {
      "type": "string"
    }
  }
}
